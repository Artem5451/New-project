class Program
{
    static void Main(string[] args)
    {
        double x;
        Console.Write("Ã— = ");
        x = Convert.ToDouble(Console.ReadLine());
        double y = (Math.Tan(Math.Pow(Math.Pow((x - 1), 2), 1 / 3)) + Math.Pow(Math.E ,3-3*x)) / (2 * Math.Log(10 + (Math.Abs(1 - Math.Pow(x,2) )))) * (3-x);
        Console.WriteLine("{0:0.00000}", y);
        Console.ReadKey();
    }
}
